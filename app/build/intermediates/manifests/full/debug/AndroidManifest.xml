<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.buscaminas"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="21" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.buscaminas.Inicio"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.buscaminas.Cuatro"
            android:label="@string/title_activity_cuatro" >
        </activity>

        <service android:name="com.example.buscaminas.Accion" />

        <activity
            android:name="com.example.buscaminas.Cinco"
            android:label="@string/title_activity_cinco" >
        </activity>
        <activity
            android:name="com.example.buscaminas.Seis"
            android:label="@string/title_activity_seis" >
        </activity>
    </application>

</manifest>